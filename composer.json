{
  "name": "kooditorm/hyperf-ddd",
  "description": "The Core Package of Domain Driven Architecture Based on hyperf System",
  "keywords": [
    "Kooditorm",
    "hyperf",
    "hyperf-ddd",
    "ddd",
    "Domain Driven Design"
  ],
  "homepage": "https://github.com/Kooditorm/hyperf-ddd",
  "license": "MIT",
  "author": [
    {
      "name": "oswin hu",
      "email": "oswin-hu@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "hyperf/di": "~v2.2.0",
    "hyperf/db-connection": "~v2.2.0",
    "hyperf/http-server": "~v2.2.0",
    "hyperf/http-message": "~v2.2.0",
    "hyperf/snowflake": "~v2.2.0",
    "hyperf/command": "~v2.2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.3.3"
  },
  "autoload": {
    "psr-4": {
